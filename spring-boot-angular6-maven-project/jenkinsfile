pipeline {
    agent any
       

   stage("Compile Stage"){

       steps{
           withMaven(Maven: 'apache-maven-3.5.0')
           sh 'mvn clean compile'

       }
   }

stage("Testing Stage"){

       steps{
           withMaven(Maven: 'apache-maven-3.5.0')
           sh 'mvn clean test'

       }
   }



}